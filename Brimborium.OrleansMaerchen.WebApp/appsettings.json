{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Orleans": {
    "ClusterId": "ClusterId",
    "ServiceId": "ServiceId",
    /* SiloPort */
    /* GatewayPort */
    /* AdvertisedIPAddress */
    /* GatewayListeningEndpoint */
    /* SiloListeningEndpoint */
    "FileGrainStorage": {
      "RootDirectory": ""
    }
  },
  "UseHttpsRedirection": false,
  "RateLimit": {
    "IpLimiter": {
      "IsEnabled": false,
      "QueueLimit": 100,
      "ReplenishmentPeriod": "00:01:00",
      "TokenLimit": 100,
      "TokensPerPeriod": 1
    },
    "LimiterUserName": {
      "IsEnabled": false,
      "PermitLimit": 100,
      "QueueLimit": 100,
      "SegmentsPerWindow": 4,
      "Window": "00:02:00"
    },
    "LimiterAnonymous": {
      "IsEnabled": false,
      "PermitLimit": 100,
      "QueueLimit": 100,
      "SegmentsPerWindow": 4,
      "Window": "00:02:00"
    }
  }
}
